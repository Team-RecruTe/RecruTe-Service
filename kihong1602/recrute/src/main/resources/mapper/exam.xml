<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="exam">
  <select id="getRecruitTitle" parameterType="Integer" resultType="ExamInfoDTO">
    select title, start_date as startDate, end_date as endDate from recruitment
    where id = #{recruitId}
  </select>
</mapper>